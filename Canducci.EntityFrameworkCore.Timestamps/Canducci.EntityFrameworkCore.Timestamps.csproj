<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <RepositoryUrl>https://github.com/fulviocanducci/Canducci.EntityFrameworkCore.Timestamps</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>Timestamps CreatedAt UpdatedAt</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Timestamps CreatedAt UpdatedAt</Description>
    <Authors>fulviocanducci</Authors>
    <PackageReleaseNotes>Entity Framework Core With Timestamps (CreatedAt &amp;&amp; UpdatedAt)</PackageReleaseNotes>
    <PackageIcon>base-ico.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="5.0.5" />
  </ItemGroup>

  <ItemGroup>
    <None Include="base-ico.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
